{
  "name": "parkersprouse.github.io",
  "private": true,
  "version": "1.0.0",
  "browserslist": "defaults",
  "engines": {
    "node": "^18",
    "pnpm": "^8"
  },
  "scripts": {
    "build": "node ./bin/build.mjs",
    "lint:css": "stylelint src/*.css",
    "lint:css:fix": "stylelint --fix src/*.css",
    "lint:js": "eslint --ext .js src/",
    "lint:js:fix": "eslint --fix --ext .js src/",
    "start": "pnpm run build && esbuild --servedir=dist"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.8",
    "@fontsource/rampart-one": "^5.0.11",
    "sanitize.css": "^13.0.0"
  },
  "devDependencies": {
    "@shopify/eslint-plugin": "^43.0.0",
    "autoprefixer": "^10.4.15",
    "esbuild": "^0.19.3",
    "eslint": "^8.50.0",
    "eslint-plugin-github": "^4.10.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "postcss": "^8.4.29",
    "postcss-preset-env": "^9.1.4",
    "stylelint": "^15.10.3",
    "stylelint-config-clean-order": "^5.2.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-use-nesting": "^4.1.0"
  }
}
